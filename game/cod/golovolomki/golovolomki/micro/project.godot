config_version=4

[application]

config/name="micro"
run/main_scene="res://micro.tscn"
config/icon="res://icon.svg"
config/features=PoolStringArray( "4.0", "GL Compatibility" )

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
